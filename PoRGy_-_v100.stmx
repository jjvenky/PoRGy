            <display x="412" y="94" color="blue" />
        </aux>
        <aux name="Tpoly">
            <eqn>Tpoly5*LocalHour^5 + Tpoly4*LocalHour^4 + Tpoly3*LocalHour^3 + Tpoly2*LocalHour^2 + Tpoly1*LocalHour^1 + Tpoly0</eqn>
            <display x="774" y="183" color="blue">
